    "actions = np.array(['chat', 'crying', 'free', 'help', 'name', 'phone number', 'room', 'serve', 'sleep', 'today', 'train', 'you'])\n",
    "\n",
    "# Thirty videos worth of data\n",
    "no_sequences = 30\n",
    "\n",
    "# Videos are going to be 30 frames in length\n",
    "sequence_length = 30\n",
    "\n",
    "# Folder start\n",
    "start_folder = 30"
